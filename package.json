{
  "name": "formbuilder-lhcforms",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "install-and-build": "source bashrc.formbuilder && npm install && npm run clean-build",
    "clean-build": "source bashrc.formbuilder && npm ci && source bashrc.formbuilder && npm run build",
    "build": "ng build",
    "test": "npm run unit-test && npm run e2e",
    "unit-test": "ng test --watch=false --browsers ChromeHeadless",
    "lint": "ng lint",
    "e2e": "node ./node_modules/.bin/webdriver-manager clean && node ./node_modules/.bin/webdriver-manager update --versions.chrome  $(google-chrome --version |awk '{print $3}') && ng e2e --webdriver-update=false",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.4",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "~10.2.4",
    "@angular/compiler": "~10.2.4",
    "@angular/core": "~10.2.4",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "~10.2.4",
    "@angular/localize": "^10.2.4",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "~10.2.4",
    "@angular/platform-browser-dynamic": "~10.2.4",
    "@angular/router": "~10.2.4",
    "@circlon/angular-tree-component": "^10.0.2",
    "@fortawesome/angular-fontawesome": "^0.8.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fullcalendar/core": "^5.5.1",
    "@json-editor/json-editor": "^2.5.1",
    "@ng-bootstrap/ng-bootstrap": "^8.0.4",
    "bootstrap": "^4.5.3",
    "chart.js": "^2.9.4",
    "fhirclient": "^2.3.11",
    "jexl": "^2.3.0",
    "ngx-schema-form": "2.6.0",
    "quill": "^1.3.7",
    "rxjs": "^6.6.3",
    "tslib": "^2.1.0",
    "z-schema": "^4.2.3",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.1",
    "@angular/cli": "~10.2.1",
    "@angular/compiler-cli": "~10.2.4",
    "@angular/language-service": "~10.2.4",
    "@types/jasmine": "~3.6.3",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.14.22",
    "ajv": "^6.12.6",
    "ajv-cli": "^3.3.0",
    "codelyzer": "^6.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "json-pointer": "^0.6.1",
    "karma": "^6.0.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "~7.0.0",
    "traverse": "^0.6.6",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.3",
    "typescript": "^4.0.5"
  }
}
