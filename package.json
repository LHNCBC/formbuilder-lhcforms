{
  "name": "formbuilder-lhcforms",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 9030",
    "start-public": "node ./bin/start-public.js",
    "install-and-build": "source ./bashrc.formbuilder && npm install && npm run clean-build",
    "clean-build": "source ./bashrc.formbuilder && npm ci && source ./bashrc.formbuilder && npm run build",
    "build": "ng build",
    "test": "source ./bashrc.formbuilder && npm run unit-test && npm run ci:cy-run",
    "unit-test": "ng test --watch=false --browsers ChromeHeadless",
    "lint": "ng lint",
    "cy:verify": "cypress verify",
    "cy:open": "cypress open",
    "cy:run": "source ./bashrc.formbuilder && cypress run --browser chrome",
    "ci:cy-run": "start-server-and-test ci:start-server http://localhost:9031 cy:run",
    "ci:cy-open": "start-server-and-test ci:start-server http://localhost:9031 cy:open",
    "ci:start-server": "angular-http-server --path ./dist/formbuilder-lhcforms -p 9031",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.14",
