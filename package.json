{
  "name": "formbuilder-lhcforms",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 8100",
    "install-and-build": "source ./bashrc.formbuilder && npm install && npm run clean-build",
    "clean-build": "source ./bashrc.formbuilder && npm ci && source ./bashrc.formbuilder && npm run build",
    "build": "ng build",
    "test": "npm run unit-test && npm run cy-run",
    "unit-test": "ng test --watch=false --browsers ChromeHeadless",
    "lint": "ng lint",
    "cy-open": "cypress open",
    "cy-run": "cypress run --browser chrome",
    "e2e": "node ./node_modules/.bin/webdriver-manager clean && node ./node_modules/.bin/webdriver-manager update --versions.chrome  $(google-chrome --version |awk '{print $3}') && ng e2e --webdriver-update=false --port 8101",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.14",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~11.2.14",
    "@angular/localize": "^11.2.14",
    "@angular/material": "^11.2.13",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/router": "~11.2.14",
    "@circlon/angular-tree-component": "^11.0.4",
    "@fortawesome/angular-fontawesome": "^0.8.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fullcalendar/core": "^5.7.0",
    "@json-editor/json-editor": "^2.5.4",
    "@ng-bootstrap/ng-bootstrap": "^9.1.1",
    "autocomplete-lhc": "^18.1.3",
    "bootstrap": "^4.6.0",
    "chart.js": "^2.9.4",
    "fast-copy": "^2.1.1",
    "fhirclient": "^2.3.11",
    "jexl": "^2.3.0",
    "jquery": "^3.6.0",
    "json-schema-traverse": "^1.0.0",
    "math-to-fhirpath": "NLMLHC/math-to-fhirpath",
    "ngx-schema-form": "2.6.5",
    "popper.js": "^1.16.1",
    "rule-editor": "github:NLMLHC/rule-editor#ng-rule-editor",
    "rxjs": "^6.6.3",
    "tslib": "^2.1.0",
    "z-schema": "^4.2.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.13",
    "@angular/cli": "~11.2.13",
    "@angular/compiler-cli": "~11.2.14",
    "@angular/language-service": "~11.2.14",
    "@types/hapi": "18.0.6",
    "@types/jasmine": "~3.6.3",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.14.22",
    "ajv": "^6.12.6",
    "ajv-cli": "^3.3.0",
    "codelyzer": "^6.0.1",
    "cypress": "^8.3.0",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~6.0.0",
    "json-pointer": "^0.6.1",
    "karma": "^6.0.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
